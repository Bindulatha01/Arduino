/* Arithmatic Operators */

int a,b,c,d,e,f,g;

void setup() {
   Serial.begin(9600);
   a=5,b=7;
   c=a+b;
   d=a-b;
   e=a*b;
   f=a/b;
   g=a%b;
   Serial.println(c);
   Serial.println(d);
   Serial.println(e);
   Serial.println(f);
   Serial.println(g);
}

void loop() {

}

